      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_bullet"
          },
          "named": true,
          "value": "list_bullet"
        },
        {
          "type": "SYMBOL",
          "name": "line"
        }
      ]
    },
    "_bullet": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\*|\\+|-|•|‣|⁃"
          },
          {
            "type": "PATTERN",
            "value": "\\s"
          }
        ]
      }
    },
    "enumerated_list": {
      "type": "REPEAT1",
