      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[0-9]+|[a-z]|[A-Z]|[IVXLCDM]+|[ivxlcdm]+|#"
              },
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "PATTERN",
                "value": "\\s"
              }
            ]
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "("
              },
              {
                "type": "PATTERN",
                "value": "[0-9]+|[a-z]|[A-Z]|[IVXLCDM]+|[ivxlcdm]+|#"
              },
              {
                "type": "STRING",
                "value": ")"
              },
              {
                "type": "PATTERN",
                "value": "\\s"
              }
            ]
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[0-9]+|[a-z]|[A-Z]|[IVXLCDM]+|[ivxlcdm]+|#"
              },
              {
                "type": "STRING",
                "value": ")"
              },
              {
                "type": "PATTERN",
                "value": "\\s"
              }
            ]
          }
        }
      ]
    },
